# SOS Store Manager Environment Variables Example
# Copy this to .env and fill in your values

# Environment
NODE_ENV=development
PORT=3000

# Database
DATABASE_URL=postgresql://user:password@localhost:5432/sos_store_db
REDIS_URL=redis://localhost:6379

# Shopify App
SHOPIFY_API_KEY=your_shopify_api_key
SHOPIFY_API_SECRET=your_shopify_api_secret
SHOPIFY_WEBHOOK_SECRET=your_webhook_secret

# IP Lookup Services
ABUSEIPDB_API_KEY=
IPQUALITYSCORE_API_KEY=
IPINFO_API_KEY=

# Email Verification Services
EMAILREP_API_KEY=
ZEROBOUNCE_API_KEY=
ABSTRACTAPI_EMAIL_KEY=

# Phone Validation Services
NUMVERIFY_API_KEY=
ABSTRACTAPI_PHONE_KEY=
VERIPHONE_API_KEY=

# AI Services
OPENAI_API_KEY=

# Security
ENCRYPTION_KEY=
JWT_SECRET=